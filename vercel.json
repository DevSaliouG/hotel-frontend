{
  "version": 2,
  "builds": [
    {
      "src": "dist/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
     {
      "src": "/api/(.*)",
      "dest": "https://hotel-backend-pely.onrender.com/api/$1"
    },
    {
      "src": "/.*",
      "dest": "/index.html",
      "status": 200
    }
  ],


}